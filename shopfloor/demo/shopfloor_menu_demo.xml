<odoo noupdate="1">
    <record id="shopfloor_menu_single_pallet_transfer" model="shopfloor.menu">
        <field name="name">Single Pallet Transfer</field>
        <field name="sequence">20</field>
        <field name="allow_move_create" eval="1" />
        <field name="scenario">single_pack_transfer</field>
        <field
            name="picking_type_ids"
            eval="[(4, ref('shopfloor.picking_type_single_pallet_transfer_demo'))]"
        />
    </record>
    <record id="shopfloor_menu_zone_picking" model="shopfloor.menu">
        <field name="name">Zone Picking</field>
        <field name="sequence">35</field>
        <field name="scenario">zone_picking</field>
        <field
            name="picking_type_ids"
            eval="[(4, ref('shopfloor.picking_type_zone_picking_demo'))]"
        />
    </record>
    <record id="shopfloor_menu_cluster_picking" model="shopfloor.menu">
        <field name="name">Cluster Picking</field>
        <field name="sequence">30</field>
        <field name="scenario">cluster_picking</field>
        <field
            name="picking_type_ids"
            eval="[(4, ref('shopfloor.picking_type_cluster_picking_demo'))]"
        />
    </record>
    <record id="shopfloor_menu_checkout" model="shopfloor.menu">
        <field name="name">Checkout</field>
        <field name="sequence">40</field>
        <field name="scenario">checkout</field>
        <field
            name="picking_type_ids"
            eval="[(4, ref('shopfloor.picking_type_checkout_demo'))]"
        />
    </record>
    <record id="shopfloor_menu_delivery" model="shopfloor.menu">
        <field name="name">Delivery</field>
        <field name="sequence">50</field>
        <field name="scenario">delivery</field>
        <field
            name="picking_type_ids"
            eval="[(4, ref('shopfloor.picking_type_delivery_demo'))]"
        />
    </record>
    <record id="shopfloor_menu_location_content_transfer" model="shopfloor.menu">
        <field name="name">Location Content Transfer</field>
        <field name="sequence">60</field>
        <field name="allow_move_create" eval="1" />
        <field name="scenario">location_content_transfer</field>
        <field
            name="picking_type_ids"
            eval="[(4, ref('shopfloor.picking_type_location_content_transfer_demo'))]"
        />
    </record>
</odoo>
