<odoo>

    <record model="ir.ui.view" id="stock_release_channel_form_view">
        <field name="model">stock.release.channel</field>
        <field
            name="inherit_id"
            ref="stock_release_channel.stock_release_channel_form_view"
        />
        <field name="arch" type="xml">
            <xpath expr="//notebook" position="inside">
                <page string="Shipment loading and departure Settings" name="loading">
                    <group>
                        <group>
                            <field name="planned_arrival_time" widget="float_time" />
                            <field name="planned_departure_time" widget="float_time" />
                        </group>
                        <group>
                            <field name="planned_arrival_datetime" />
                            <field name="planned_departure_datetime" />
                        </group>
                    </group>
                </page>
            </xpath>
        </field>
    </record>



</odoo>
