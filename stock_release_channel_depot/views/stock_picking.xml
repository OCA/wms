<?xml version="1.0" encoding="UTF-8" ?>
<odoo>
    <record id="view_picking_form" model="ir.ui.view">
        <field name="name">stock.picking.form (in stock_release_channel_depot)</field>
        <field name="model">stock.picking</field>
        <field name="inherit_id" ref="stock_release_channel.view_picking_form" />
        <field name="arch" type="xml">
            <field name="release_channel_id" position="after">
                <field name="depot_id" />
            </field>
        </field>
    </record>

    <record id="vpicktree_view_tree" model="ir.ui.view">
        <field name="name">stock.picking.delivery.tree</field>
        <field name="model">stock.picking</field>
        <field name="inherit_id" ref="stock.vpicktree" />
        <field name="arch" type="xml">
            <xpath expr="//field[@name='state']" position="before">
                <field name="depot_id" optional="hide" />
            </xpath>
        </field>
    </record>
</odoo>
