<odoo>

  <record model="ir.ui.view" id="stock_device_type_line_tree_view">
    <field name="name">stock.device.type.line.tree (in stock_picking_batch_creation)</field>
    <field name="model">stock.device.type.line</field>
    <field name="arch" type="xml">
      <tree>
        <field name="name" />
        <field name="stock_device_type_id" />
        <field name="sequence" />
      </tree>
    </field>
  </record>

  <record model="ir.ui.view" id="stock_device_type_line_form_view">
    <field name="name">stock.device.type.line.form (in stock_picking_batch_creation)</field>
    <field name="model">stock.device.type.line</field>
    <field name="arch" type="xml">
      <form>
        <sheet>
            <group>
                <field name="name" />
                <field name="stock_device_type_id" />
                <field name="sequence" />
            </group>
        </sheet>
      </form>
    </field>
  </record>
  <act_window
    id="stock_device_type_line_act_window"
    name="Stock Device Type Line"
    res_model="stock.device.type.line"
    view_mode="tree,form"
  />
  <menuitem
    id="menu_stock_device_type_line"
    action="stock_device_type_line_act_window"
    parent="stock_picking_batch_creation.menu_stock_device_types"
    groups="stock.group_stock_manager"
    sequence="2"
  />
</odoo>
