<odoo>

  <record model="ir.ui.view" id="stock_device_type_tree_view">
    <field name="name">stock.device.type.tree (in stock_picking_batch_creation)</field>
    <field name="model">stock.device.type</field>
    <field name="arch" type="xml">
      <tree>
        <field name="name" />
        <field name="min_volume" />
        <field name="max_volume" />
        <field name="max_weight" />
        <field name="nbr_bins" />
      </tree>
    </field>
  </record>

  <record model="ir.ui.view" id="stock_device_type_form_view">
    <field name="name">stock.device.type.form (in stock_picking_batch_creation)</field>
    <field name="model">stock.device.type</field>
    <field name="arch" type="xml">
      <form string="Stock Picking Waves">
        <field name="name" />
        <field name="min_volume" />
        <field name="max_volume" />
        <field name="max_weight" />
        <field name="nbr_bins" />
        <field name="stock_device_type_line_ids">
        </field>
      </form>
    </field>
  </record>
  <act_window
    id="stock_device_type_act_window"
    name="Stock Device Type"
    res_model="stock.device.type"
    view_type="form"
    view_mode="tree,form"
  />
  <menuitem
    id="menu_stock_device_type"
    action="stock_device_type_act_window"
    parent="stock.menu_stock_config_settings"
    groups="stock.group_stock_manager"
    sequence="100"
  />
</odoo>
