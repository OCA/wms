<odoo>
    <record model="ir.ui.view" id="view_picking_type_form">
        <field name="name">Stock full location reservation on assign</field>
        <field name="model">stock.picking.type</field>
        <field
            name="inherit_id"
            ref="stock_full_location_reservation.view_picking_type_form"
        />
        <field name="arch" type="xml">
            <field name="is_full_location_reservation_visible" position="after">
                <field
                    name="auto_full_location_reservation_on_assign"
                    attrs='{"invisible": [("code", "not in", ["internal"])]}'
                />
            </field>
        </field>
    </record>
</odoo>
