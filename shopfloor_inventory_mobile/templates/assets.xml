<odoo>
    <template
        id="shopfloor_app_assets"
        inherit_id="shopfloor_mobile.shopfloor_app_assets"
    >
        <script id="script_scenario_zone_picking" position="after">
            <t
                t-set="mod_version"
                t-value="get_version('shopfloor_inventory_mobile')"
            />
            <script
                id="script_component_batch_picking_detail"
                t-attf-src="/shopfloor_inventory_mobile/static/wms/src/components/detail/inventory_detail.js?v=#{mod_version}"
                type="module"
            />
            <script
                id="script_component_batch_picking_detail"
                t-attf-src="/shopfloor_inventory_mobile/static/wms/src/components/detail/inventory_line_detail.js?v=#{mod_version}"
                type="module"
            />
            <script
                id="script_scenario_inventory"
                t-attf-src="/shopfloor_inventory_mobile/static/wms/src/scenario/inventory.js?v=#{mod_version}"
                type="module"
            />
        </script>
    </template>
</odoo>
